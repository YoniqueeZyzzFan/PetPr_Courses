{"remainingRequest":"C:\\Data_ik\\pet_project\\client\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\Data_ik\\pet_project\\client\\src\\components\\Cours.vue?vue&type=style&index=0&lang=css&","dependencies":[{"path":"C:\\Data_ik\\pet_project\\client\\src\\components\\Cours.vue","mtime":1660635494498},{"path":"C:\\Data_ik\\pet_project\\client\\node_modules\\css-loader\\dist\\cjs.js","mtime":1660285778089},{"path":"C:\\Data_ik\\pet_project\\client\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":1660285809813},{"path":"C:\\Data_ik\\pet_project\\client\\node_modules\\postcss-loader\\src\\index.js","mtime":1660285790244},{"path":"C:\\Data_ik\\pet_project\\client\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1660285767018},{"path":"C:\\Data_ik\\pet_project\\client\\node_modules\\vue-loader\\lib\\index.js","mtime":1660285801241}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["Cours.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAwJA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Cours.vue","sourceRoot":"src/components","sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"row\">\n      <div class=\"col-sm-10\">\n        <h1>MOOCs</h1>\n        <hr><br>\n        <button type=\"button\" class=\"btn btn-primary\"\n           v-if=\"showCourses || showCourseMaterial\" v-on:click=\"getMoocs\">Вернуться к ресурсам\n        </button>\n        <table v-if=\"showMoocs\" class=\"table table-dark\">\n          <thead>\n          <tr>\n            <th class=\"text-center\" scope=\"col\" colspan=\"100%\" >Названия ресурсов</th>\n          </tr>\n          </thead>\n          <tbody>\n          <tr v-for=\"(mooc, index) in moocs\" :key=\"index\">\n            <td><button type=\"button\" class=\"btn btn-light btn-outline-secondary btn-block\"\n                        v-on:click=\"getCourses(mooc)\" :value=\"mooc\">{{ mooc }}\n            </button></td>\n          </tr>\n          </tbody>\n        </table>\n\n        <table v-if=\"showCourses\" class=\"table table-hover table-bordered table-dark\">\n          <thead>\n          <tr>\n            <th class=\"text-center\" scope=\"col\" colspan=\"100%\">Доступные курсы</th>\n          </tr>\n          </thead>\n          <tbody>\n          <tr class=\"text-center\" v-for=\"(course, index) in courses\" :key=\"index\">\n            <td>\n              <button class=\"btn btn-light btn-outline-secondary\"\n                      v-on:click=\"getCourseMaterial(course.acronym)\" :value=\"course.acronym\">\n                {{course.acronym}} : {{ course.title }}\n              </button>\n            </td>\n            <td class=\"text-center\" v-for=\"(week, j) in course.weeks\" :key=\"j\">{{week}}</td>\n          </tr>\n          </tbody>\n        </table>\n\n        <div v-if=\"showCourseMaterial\">\n          <video class=\"embed-responsive-item\"\n                 v-if=\"video\" :key=\"video\" controls width=\"500\" height=\"400\">\n            <source :src=\"video\">\n          </video>\n          <ul role=\"button\" class=\"list-group\">\n            <details open v-for=\"(week, index) in courseMaterial.weeks_info\" :key=\"index\">\n              <summary class=\"btn bg-dark text-white\">{{week.week_title}}</summary>\n              <ul class=\"list-group\">\n                <li v-for=\"(video, j) in week.videos\" :key=\"j\">\n                  <button class=\"btn bg-secondary text-white\"\n                          v-on:click=\"getVideo(week.week_title, video)\"> {{ video }} </button></li>\n              </ul>\n            </details>\n          </ul>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script type=\"text/javascript\">\nimport axios from 'axios';\nimport \"C:\\\\Data_ik\\\\pet_project\\\\client\\\\node_modules\\\\bootstrap\\\\scss\\\\bootstrap.scss\"\n\nexport default {\n  data() {\n    return {\n      video: '',\n      moocs: [],\n      courses: [],\n      courseMaterial: [],\n      showMoocs: true,\n      showCourses: false,\n      showCourseMaterial: false,\n      constructedString: '',\n    };\n  },\n  methods: {\n    getMoocs() {\n      const path = 'http://localhost:5000/';\n      axios.get(path)\n        .then((res) => {\n          console.log(res);\n          this.moocs = res.data.moocs;\n          this.showMoocs = true;\n          this.showCourses = false;\n          this.showCourseMaterial = false;\n          this.constructedString = '';\n          this.video = '';\n        })\n        .catch((error) => {\n          // eslint-disable-next-line\n          console.error(error);\n        });\n    },\n    getCourses(moocTitle) {\n      const path = `http://localhost:5000/${moocTitle}`;\n      console.log(path);\n      axios.get(path)\n        .then((res) => {\n          console.log(res);\n          this.courses = res.data.courses;\n          this.showMoocs = false;\n          this.showCourses = true;\n          this.showCourseMaterial = false;\n          this.constructedString = moocTitle;\n        })\n        .catch((error) => {\n          // eslint-disable-next-line\n          console.error(error);\n        });\n    },\n    getCourseMaterial(courseTitle) {\n      const path = `http://localhost:5000/${this.constructedString}/${courseTitle}`;\n      console.log(path);\n      axios.get(path)\n        .then((res) => {\n          console.log(res);\n          this.courseMaterial = res.data.courseMaterial;\n          this.showMoocs = false;\n          this.showCourses = false;\n          this.showCourseMaterial = true;\n          this.constructedString += `/${courseTitle}`;\n        })\n        .catch((error) => {\n          // eslint-disable-next-line\n          console.error(error);\n        });\n    },\n    getVideo(weekTitle, videoTitle) {\n      const path = `./${this.constructedString}/${weekTitle}/${videoTitle}`;\n      const xhr = new XMLHttpRequest();\n      xhr.open('HEAD', path, false);\n      xhr.send();\n      if (xhr.status === 404) {\n        console.log('sorry, file not found');\n      } else {\n        this.video = path;\n      }\n    },\n  },\n  created() {\n    this.getMoocs();\n  },\n};\n</script>\n\n<style>\n/* CSS */\n.btn-rounded {\n  border-radius: 19px;\n  padding-top: 3px;\n  padding-bottom: 3px;\n  padding-left: 3px;\n}\n</style>\n"]}]}