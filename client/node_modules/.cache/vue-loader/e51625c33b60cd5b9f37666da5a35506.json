        <button type=\"button\"\n                          class=\"btn tn-warning btn-sm\"\n                          @click=\"console.log('123')\"\n                  >{{k}}</button>\n                </td>\n              </tr>\n            </div>\n          </td>\n          <td v-if=\"4<=weeks_numb\">\n            <button type=\"button\" class=\"btn btn-warning btn-sm\"\n                    @click=\"wv4=!wv4\">Week 4 - Show/Hide</button>\n            <div v-if=\"wv4\">\n              <tr v-for=\"k in week_items['4']\" :key=\"k\">\n                <td class=\"btn-group\" role=\"group\">\n                  <button type=\"button\"\n                          class=\"btn tn-warning btn-sm\"\n                          @click=\"console.log('123')\"\n                  >{{k}}</button>\n                </td>\n              </tr>\n            </div>\n          </td>\n          </tbody>\n        </table>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      courses: [],\n      week_items: [{}],\n      curr_site: '',\n      wv1: false,\n      wv2: false,\n      wv3: false,\n      wv4: false,\n      curr_course: '',\n      weeks_numb: 0,\n      addBookForm: {\n        title: '',\n      },\n      message: '',\n      showSites: true,\n      showCourses: false,\n      showCourse: false,\n    };\n  },\n  components: {\n  },\n  methods: {\n    getCourses() {\n      const path = 'http://localhost:5000/';\n      axios.get(path)\n        .then((res) => {\n          this.courses = res.data.sites;\n        })\n        .catch((error) => {\n          // eslint-disable-next-line\n          console.error(error);\n        });\n    },\n    goCourses(site) {\n      this.showCourses = true;\n      this.showSites = false;\n      let path = 'http://localhost:5000/';\n      path += site;\n      this.curr_site = site;\n      axios.get(path)\n        .then((res) => {\n          this.courses = res.data;\n        })\n